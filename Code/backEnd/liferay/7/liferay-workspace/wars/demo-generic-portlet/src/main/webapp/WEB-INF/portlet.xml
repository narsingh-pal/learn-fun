<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>demo-generic-portlet</portlet-name>
		<display-name>demo-generic-portlet Portlet</display-name>
		<portlet-class>com.nsp.j2ee.liferay.demo.genericportlet.DemoGenericPortlet</portlet-class>
        <init-param>
            <name>view-template</name>
            <value>/jsp/genericdemo/view.jsp</value>
        </init-param>
		<expiration-cache>0</expiration-cache>

		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>demo-generic-portlet Portlet</title>
			<short-title>demo-generic-portlet Portlet</short-title>
			<keywords>demo-generic-portlet Portlet</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>